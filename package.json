{
  "name": "tella-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:css": "postcss styles/globals.scss -o public/tailwind.css --watch",
    "build": "next build",
    "build:css": "cross-env NODE_ENV=production postcss styles/globals.scss -o public/tailwind.css",
    "start": "next start",
    "lint": "eslint './**/*.{ts,tsx}'",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "storybook": "start-storybook -p 9011 -s ./public -c ./.storybook/ && npm run dev:css",
    "build-storybook": "build-storybook -c .storybook -o .out -s ./public && npm run build:css",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "chromatic": "chromatic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.9",
    "autoprefixer": "^9",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "fontsource-lato": "^3.0.9",
    "next": "10.0.3",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.29.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@storybook/preset-typescript": "^3.0.0",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.4.0",
    "css-loader": "^5.0.1",
    "cypress": "^6.0.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2"
  }
}
